
pipeline {
	agent any
	
	stages {
		stage('Copy artifact') { 
			steps {
				copyArtifacts filter: 'golambda.zip', fingerprintArtifacts: true, projectName: 'golambda', selector: lastSuccessful()
			}
		}
		stage('Create lambda with docker') {
			steps {
				sh "docker run -v /home/vagrant/.aws:/root/.aws -v $HOME/workspace/Deploy:/aws -t amazon/aws-cli lambda create-function --function-name golambda \
--zip-file fileb://golambda.zip --handler golambda --runtime go1.x \
--role arn:aws:iam::786136325720:instance-profile/lambda"
			}
		}
	}
}
